{
	"useTabs": true,
	"tabWidth": 4,
	"printWidth": 160,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"endOfLine": "lf",
	"trailingComma": "none",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"quoteProps": "consistent",
	"arrowParens": "always",
	"singleAttributePerLine": false,
	"requirePragma": false,
	"insertPragma": false,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"proseWrap": "preserve",
	"vueIndentScriptAndStyle": true
}
